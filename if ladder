import java.util.Scanner;

        public class if_elseif_ladder {
            public static void main(String[] args) {
                Scanner n = new Scanner(System.in);
                System.out.println("Enter first  number: ");
                int a = n.nextInt();
                System.out.println("Enter second number: ");
                int b = n.nextInt();
                if(a+b>10){
                    System.out.println("The ladder is greater than 10");
                }
                else if (a+b<10)
                {System.out.println("The ladder is lesser than 10");}
                else if(a+b==10)
                {
                    System.out.println("The ladder is equals than 10");
                }
                else{
                    System.out.println("Else statement execution");
                }

            }
        }
