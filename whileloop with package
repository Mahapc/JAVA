import java.util.Scanner;
public class while_loop {
    public static void main(String[] args) {
        int i=0;
        System.out.println("Enter number") ;
        Scanner input= new Scanner(System.in);
        int n = input.nextInt();
        while(i<=n){
            System.out.println(i);
            i=i+2;
        }
    }
}
